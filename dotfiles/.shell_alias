function path {
    echo $PATH | tr ':' '\n'
}
function addpath {
    if [[ ! "$PATH:" =~ "$1:" ]]; then
        if [[ $2 == 'prefix' ]]; then
            export PATH=$1:$PATH
        else
            export PATH=$PATH:$1
        fi
    fi
}
